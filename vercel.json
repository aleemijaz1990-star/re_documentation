{
  "rewrites": [
    {
      "source": "/_next/:path*",
      "destination": "/_next/:path*"
    },
    {
      "source": "/static/:path*",
      "destination": "/static/:path*"
    },
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/:path*",
      "destination": "/api/docs"
    }
  ]
}