{
  "rewrites": [
    {
      "source": "/assets/:path*",
      "destination": "/assets/:path*"
    },
    {
      "source": "/__docusaurus/:path*",
      "destination": "/__docusaurus/:path*"
    },
 {
      "source": "/_next/:path*",
      "destination": "/_next/:path*"
    },
    {
      "source": "/static/:path*",
      "destination": "/static/:path*"
    },

    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },

    {
      "source": "/:path*",
      "destination": "/api/docs"
    }
  ]
}